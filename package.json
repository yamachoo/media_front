{
  "name": "media_front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint:eslint": "eslint --ext .js,.ts,.jsx,.tsx --ignore-path .gitignore --cache .",
    "lint:prettier": "prettier --ignore-path .gitignore --write .",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --ignore-path .gitignore --cache --fix",
    "*.{js,ts,jsx,tsx,css,md,json,yml}": "prettier --ignore-path .gitignore --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "2.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/user-event": "13.2.1",
    "@types/jest": "27.0.0",
    "@types/node": "16.6.0",
    "@types/react": "17.0.17",
    "@types/react-dom": "17.0.9",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.24.0",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-testing-library": "4.10.1",
    "husky": "7.0.1",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2",
    "typescript": "4.3.5"
  }
}
